<!DOCTYPE html>
<html class="no-js" lang="en">
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=Edge" />
    <meta
      content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no"
      name="viewport"
    />

    <title>Dashboard - Tenda :: Tendaku</title>
    <!-- Favicon-->
    <link rel="icon" href="img/favicon.png" type="image/x-icon" />

    <link rel="stylesheet" href="plugins/bootstrap/css/bootstrap.min.css" />

    <!-- Custom Css -->
    <link rel="stylesheet" href="admincss/style.min.css" />
  </head>

  <body class="theme-blush">
    <!-- Page Loader -->
    <div class="page-loader-wrapper">
      <div class="loader">
        <div class="m-t-30">
          <img class="zmdi-hc-spin" src="images/loader.svg" width="48" height="48" alt="Aero" />
        </div>
        <p>Please wait...</p>
      </div>
    </div>

    <!-- Overlay For Sidebars -->
    <div class="overlay"></div>

    <div class="navbar-right">
      <ul class="navbar-nav">
        <li>
          <a href="/petugas/logout" class="mega-menu" title="Sign Out"
            ><i class="zmdi zmdi-power"></i
          ></a>
        </li>
      </ul>
    </div>
    <!-- Left Sidebar -->
    <aside id="leftsidebar" class="sidebar">
      <div class="navbar-brand">
        <button class="btn-menu ls-toggle-btn" type="button"><i class="zmdi zmdi-menu"></i></button>
        <a href="/petugas/dashboard"><span class="m-l-10">Tendaku</span></a>
      </div>
      <div class="menu">
        <ul class="list">
          <li>
            <div class="user-info">
              <a class="image" href="profile.html"
                ><img src="images/profile_av.jpg" alt="User"
              /></a>
              <div class="detail">
                <h4><%= userData.nama %></h4>
                <small>Petugas Tendaku</small>
              </div>
            </div>
          </li>
          <li>
            <a href="/petugas/dashboard"><i class="zmdi zmdi-home"></i><span>Dashboard</span></a>
          </li>

          <li class="active open">
            <a href="javascript:void(0);" class="menu-toggle"
              ><i class="zmdi zmdi-shopping-cart"></i><span>Tenda</span></a
            >
            <ul class="ml-menu">
              <li><a href="/petugas/dashboard/tenda">Tenda List</a></li>
              <li><a href="/petugas/dashboard/tenda/tambah">Tambah Tenda</a></li>
            </ul>
          </li>
          <li>
            <a href="javascript:void(0);" class="menu-toggle"
              ><i class="zmdi zmdi-grid"></i><span>Peminjaman</span></a
            >
            <ul class="ml-menu">
              <li><a href="/petugas/dashboard/peminjaman">Peminjaman List</a></li>
              <li><a href="#">Tambah Peminjaman</a></li>
            </ul>
          </li>
          <li>
            <a href="javascript:void(0);" class="menu-toggle"
              ><i class="zmdi zmdi-chart"></i><span>Statistik</span></a
            >
            <ul class="ml-menu">
              <li><a href="#">C3 Chart</a></li>
            </ul>
          </li>
        </ul>
      </div>
    </aside>
    <section class="content">
      <div class="body_scroll">
        <div class="block-header">
          <div class="row">
            <div class="col-lg-7 col-md-6 col-sm-12">
              <h2>Tendaku Dashboard</h2>
              <ul class="breadcrumb">
                <li class="breadcrumb-item">
                  <a href="/petugas/dashboard"><i class="zmdi zmdi-home"></i> Tendaku</a>
                </li>
                <li class="breadcrumb-item">Dashboard</li>
                <li class="breadcrumb-item">Tenda</li>
                <li class="breadcrumb-item active"><%= tenda.nama_tenda %></li>
              </ul>
              <button class="btn btn-primary btn-icon mobile_menu" type="button">
                <i class="zmdi zmdi-sort-amount-desc"></i>
              </button>
            </div>
          </div>
        </div>
      </div>

      <div class="container-fluid">
        <div class="row clearfix">
          <div class="col-lg-12">
            <div class="card">
              <div class="body">
                <div class="row">
                  <div class="col-xl-3 col-lg-4 col-md-12">
                    <div class="preview preview-pic tab-content">
                      <div class="tab-pane active" id="product_1">
                        <img
                          src="/api/tenda/img/<%= tenda.kode_tenda %>"
                          class="img-fluid"
                          alt=""
                        />
                      </div>
                    </div>
                    <ul class="preview thumbnail nav nav-tabs">
                      <li class="nav-item">
                        <a class="nav-link active" data-toggle="tab" href="#product_1"
                          ><img src="/api/tenda/img/<%= tenda.kode_tenda %>" alt=""
                        /></a>
                      </li>
                    </ul>
                  </div>
                  <div class="col-xl-9 col-lg-8 col-md-12">
                    <div class="product details">
                      <h3 class="product-title mb-0"><%= tenda.nama_tenda %></h3>
                      <h5 class="price mt-2">
                        Harga: <span class="col-amber"><%= tenda.tarif %></span>
                      </h5>
                      <hr />
                      <p class="product-description"><%= tenda.keterangan %></p>
                      <h6 class="sizes">
                        Ukuran:
                        <span class="size"><%= tenda.ukuran %></span>
                      </h6>
                      <h6 class="sizes">
                        Stok:
                        <span class="size"><%= tenda.stok %> Unit</span>
                      </h6>
                      <ul class="list-group" style="text-transform: capitalize;">
                        <li class="list-group-item">Kapasitas : <%= tenda.kapasitas %></li>
                        <li class="list-group-item">Bahan : <%= tenda.bahan %></li>
                        <li class="list-group-item">Berat : <%= tenda.berat %></li>
                        <li class="list-group-item">Kelengkapan : <%= tenda.kelengkapan %></li>
                        <li class="list-group-item">Durasi : <%= tenda.durasi %></li>
                      </ul>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-lg-12">
            <div class="card">
              <div class="body">
                <ul class="nav">
                  <li class="nav-item">
                    <a class="btn btn-primary mr-1" href="/petugas/dashboard/tenda">Kembali</a>
                  </li>
                  <li class="nav-item">
                    <a
                      class="btn btn-warning mr-1"
                      href="/petugas/dashboard/tenda/edit/<%= tenda.kode_tenda %>"
                      >Edit</a
                    >
                  </li>
                  <li class="nav-item">
                    <a
                      class="btn btn-danger mr-1"
                      href="/petugas/delete/tenda/<%= tenda.kode_tenda %>"
                      >Hapus</a
                    >
                  </li>
                </ul>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
    <script src="adminbundles/libscripts.bundle.js"></script>
    <!-- Lib Scripts Plugin Js -->
    <script src="adminbundles/vendorscripts.bundle.js"></script>
    <!-- Lib Scripts Plugin Js -->

    <script src="adminbundles/mainscripts.bundle.js"></script>
    <!-- Custom Js -->
  </body>
</html>
