<!DOCTYPE html>
<html class="no-js" lang="en">
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=Edge" />
    <meta
      content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no"
      name="viewport"
    />

    <title>Dashboard - Peminjaman :: Tendaku</title>
    <!-- Favicon-->
    <link rel="icon" href="img/favicon.png" type="image/x-icon" />

    <link rel="stylesheet" href="plugins/bootstrap/css/bootstrap.min.css" />

    <!-- Custom Css -->
    <link rel="stylesheet" href="admincss/style.min.css" />
  </head>

  <body class="theme-blush">
    <!-- Page Loader -->
    <div class="page-loader-wrapper">
      <div class="loader">
        <div class="m-t-30">
          <img class="zmdi-hc-spin" src="images/loader.svg" width="48" height="48" alt="Aero" />
        </div>
        <p>Please wait...</p>
      </div>
    </div>

    <!-- Overlay For Sidebars -->
    <div class="overlay"></div>

    <div class="navbar-right">
      <ul class="navbar-nav">
        <li>
          <a href="/petugas/logout" class="mega-menu" title="Sign Out"
            ><i class="zmdi zmdi-power"></i
          ></a>
        </li>
      </ul>
    </div>
    <!-- Left Sidebar -->
    <aside id="leftsidebar" class="sidebar">
      <div class="navbar-brand">
        <button class="btn-menu ls-toggle-btn" type="button"><i class="zmdi zmdi-menu"></i></button>
        <a href="/petugas/dashboard"><span class="m-l-10">Tendaku</span></a>
      </div>
      <div class="menu">
        <ul class="list">
          <li>
            <div class="user-info">
              <a class="image" href="profile.html"
                ><img src="images/profile_av.jpg" alt="User"
              /></a>
              <div class="detail">
                <h4><%= userData.nama %></h4>
                <small>Petugas Tendaku</small>
              </div>
            </div>
          </li>
          <li>
            <a href="/petugas/dashboard"><i class="zmdi zmdi-home"></i><span>Dashboard</span></a>
          </li>

          <li>
            <a href="javascript:void(0);" class="menu-toggle"
              ><i class="zmdi zmdi-shopping-cart"></i><span>Tenda</span></a
            >
            <ul class="ml-menu">
              <li><a href="/petugas/dashboard/tenda">Tenda List</a></li>
              <li><a href="/petugas/dashboard/tenda/tambah">Tambah Tenda</a></li>
            </ul>
          </li>
          <li class="active open">
            <a href="javascript:void(0);" class="menu-toggle"
              ><i class="zmdi zmdi-grid"></i><span>Peminjaman</span></a
            >
            <ul class="ml-menu">
              <li><a href="/petugas/dashboard/peminjaman">Peminjaman List</a></li>
              <li><a href="/petugas/dashboard/peminjaman/tambah">Tambah Peminjaman</a></li>
            </ul>
          </li>
          <li>
            <a href="javascript:void(0);" class="menu-toggle"
              ><i class="zmdi zmdi-accounts-list"></i><span>User</span></a
            >
            <ul class="ml-menu">
              <li><a href="/petugas/dashboard/user">Users List</a></li>
              <li><a href="/petugas/dashboard/user/tambah">Tambah User</a></li>
            </ul>
          </li>
          <li>
            <a href="javascript:void(0);" class="menu-toggle"
              ><i class="zmdi zmdi-accounts-list-alt"></i><span>Petugas</span></a
            >
            <ul class="ml-menu">
              <li><a href="/petugas/dashboard/ps">Petugas List</a></li>
              <li><a href="/petugas/dashboard/ps/tambah">Tambah Petugas</a></li>
            </ul>
          </li>
        </ul>
      </div>
    </aside>
    <section class="content">
      <div class="body_scroll">
        <div class="block-header">
          <div class="row">
            <div class="col-lg-7 col-md-6 col-sm-12">
              <h2>Tendaku Dashboard</h2>
              <ul class="breadcrumb">
                <li class="breadcrumb-item">
                  <a href="/petugas/dashboard"><i class="zmdi zmdi-home"></i> Tendaku</a>
                </li>
                <li class="breadcrumb-item">Dashboard</li>
                <li class="breadcrumb-item">Peminjaman</li>
                <li class="breadcrumb-item active"><%= data.main.kode %></li>
              </ul>
              <button class="btn btn-primary btn-icon mobile_menu" type="button">
                <i class="zmdi zmdi-sort-amount-desc"></i>
              </button>
            </div>
          </div>
        </div>
      </div>

      <div class="container-fluid">
        <div class="row clearfix">
          <div class="col-lg-12">
            <div class="card">
              <div class="body">
                <div class="row">
                  <div class="col-xl-3 col-lg-4 col-md-12">
                    <div class="preview preview-pic tab-content">
                      <div class="tab-pane active" id="product_1">
                        <img
                          src="/api/tenda/img/<%= data.primary.kd_tenda %>"
                          class="img-fluid"
                          alt=""
                        />
                      </div>
                    </div>
                    <ul class="preview thumbnail nav nav-tabs">
                      <li class="nav-item">
                        <a class="nav-link active" data-toggle="tab" href="#product_1"
                          ><img src="/api/tenda/img/<%= data.primary.kd_tenda %>" alt=""
                        /></a>
                      </li>
                    </ul>
                  </div>
                  <div class="col-xl-9 col-lg-8 col-md-12">
                    <div class="product details">
                      <h3 class="product-title mb-0"><%= data.tendaData.nm_tenda %></h3>
                      <h5 class="price mt-2">
                        Total: <span class="col-amber">Rp. <%= data.main.total %></span>
                      </h5>
                      <hr />
                      <p class="product-description"><%= data.main.kode %></p>
                      <h6 class="sizes">
                        Status: <% if(data.main.status.code === 0) { %>
                        <span class="size badge badge-success"><%= data.main.status.status %></span>
                        <%} else if(item.main.status.code === 1){%>
                        <span class="size badge badge-primary"><%= data.main.status.status %></span>
                        <%} else if(item.main.status.code === 2){%>
                        <span class="size badge badge-warning"><%= data.main.status.status %></span>
                        <%} else if(item.main.status.code === 3){%>
                        <span class="size badge badge-danger"><%= data.main.status.status %></span>
                        <%}else if(item.main.status.code === 4){%>
                        <span class="size badge badge-default"><%= data.main.status.status %></span>
                        <%} %>
                      </h6>
                      <h6 class="sizes">
                        Jumlah Peminjaman:
                        <span class="size"><%= data.main.jumlah %> Unit</span>
                      </h6>
                      <ul class="list-group" style="text-transform: capitalize;">
                        <li class="list-group-item">User : <%= data.userData.nm_user %></li>
                        <li class="list-group-item">
                          Petugas : <%= data.petugasData.nm_petugas %>
                        </li>
                        <li class="list-group-item">
                          Status Kode : <%= data.main.pinjam_status.stats %>
                        </li>
                        <li class="list-group-item">
                          Tgl Dikembalikan : <%= moment(data.main.tgl).format('LLL') %>
                        </li>
                      </ul>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-lg-12">
            <div class="card">
              <div class="body">
                <ul class="nav">
                  <li class="nav-item">
                    <a class="btn btn-primary mr-1" href="/petugas/dashboard/peminjaman">Kembali</a>
                  </li>
                  <% if(data.main.status.code === 1 || data.main.status.code === 3) { %>
                  <li class="nav-item">
                    <a
                      class="btn btn-success mr-1"
                      href="/petugas/terima/<%= data.primary.kd_pinjam %>"
                      >Selesai</a
                    >
                  </li>
                  %< } if(data.main.status.code === 4){ %>
                  <li class="nav-item">
                    <a
                      class="btn btn-success mr-1"
                      href="/petugas/terima/<%= data.primary.kd_pinjam %>"
                      >Terima</a
                    >
                  </li>
                  <li class="nav-item">
                    <a
                      class="btn btn-danger mr-1"
                      href="/petugas/pinjam/hapus/<%= data.primary.kd_pinjam %>"
                      >Tolak</a
                    >
                  </li>
                  <% }else{ %>
                  <li class="nav-item">
                    <a
                      class="btn btn-danger mr-1"
                      href="/petugas/pinjam/hapus/<%= data.primary.kd_pinjam %>"
                      >Hapus</a
                    >
                  </li>
                  <% } %>
                </ul>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
    <script src="adminbundles/libscripts.bundle.js"></script>
    <!-- Lib Scripts Plugin Js -->
    <script src="adminbundles/vendorscripts.bundle.js"></script>
    <!-- Lib Scripts Plugin Js -->

    <script src="adminbundles/mainscripts.bundle.js"></script>
    <!-- Custom Js -->
  </body>
</html>
