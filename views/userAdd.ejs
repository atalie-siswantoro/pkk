<!DOCTYPE html>
<html class="no-js" lang="en">
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=Edge" />
    <meta
      content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no"
      name="viewport"
    />

    <title>Dashboard - User :: Tendaku</title>
    <!-- Favicon-->
    <link rel="icon" href="img/favicon.png" type="image/x-icon" />

    <link rel="stylesheet" href="plugins/bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" href="plugins/dropify/css/dropify.min.css" />

    <!-- Custom Css -->
    <link rel="stylesheet" href="admincss/style.min.css" />
  </head>

  <body class="theme-blush">
    <div class="page-loader-wrapper">
      <div class="loader">
        <div class="m-t-30">
          <img class="zmdi-hc-spin" src="images/loader.svg" width="48" height="48" alt="Aero" />
        </div>
        <p>Please wait...</p>
      </div>
    </div>
    <!-- Overlay For Sidebars -->
    <div class="overlay"></div>

    <div class="navbar-right">
      <ul class="navbar-nav">
        <li>
          <a href="/petugas/logout" class="mega-menu" title="Sign Out"
            ><i class="zmdi zmdi-power"></i
          ></a>
        </li>
      </ul>
    </div>
    <!-- Left Sidebar -->
    <aside id="leftsidebar" class="sidebar">
      <div class="navbar-brand">
        <button class="btn-menu ls-toggle-btn" type="button"><i class="zmdi zmdi-menu"></i></button>
        <a href="/petugas/dashboard"><span class="m-l-10">Tendaku</span></a>
      </div>
      <div class="menu">
        <ul class="list">
          <li>
            <div class="user-info">
              <a class="image" href="profile.html"
                ><img src="images/profile_av.jpg" alt="User"
              /></a>
              <div class="detail">
                <h4><%= userData.nama ? userData.nama : "" %></h4>
                <small>Petugas Tendaku</small>
              </div>
            </div>
          </li>
          <li>
            <a href="/petugas/dashboard"><i class="zmdi zmdi-home"></i><span>Dashboard</span></a>
          </li>

          <li>
            <a href="javascript:void(0);" class="menu-toggle"
              ><i class="zmdi zmdi-shopping-cart"></i><span>Tenda</span></a
            >
            <ul class="ml-menu">
              <li><a href="/petugas/dashboard/tenda">Tenda List</a></li>
              <li><a href="/petugas/dashboard/tenda/tambah">Tambah Tenda</a></li>
            </ul>
          </li>
          <li>
            <a href="javascript:void(0);" class="menu-toggle"
              ><i class="zmdi zmdi-grid"></i><span>Peminjaman</span></a
            >
            <ul class="ml-menu">
              <li><a href="/petugas/dashboard/peminjaman">Peminjaman List</a></li>
              <li><a href="/petugas/dashboard/peminjaman/tambah">Tambah Peminjaman</a></li>
            </ul>
          </li>
          <li class="active open">
            <a href="javascript:void(0);" class="menu-toggle"
              ><i class="zmdi zmdi-accounts-list"></i><span>User</span></a
            >
            <ul class="ml-menu">
              <li><a href="/petugas/dashboard/user">Users List</a></li>
              <li class="active"><a href="/petugas/dashboard/user/tambah">Tambah User</a></li>
            </ul>
          </li>
          <li>
            <a href="javascript:void(0);" class="menu-toggle"
              ><i class="zmdi zmdi-accounts-list-alt"></i><span>Petugas</span></a
            >
            <ul class="ml-menu">
              <li><a href="/petugas/dashboard/ps">Petugas List</a></li>
              <li><a href="/petugas/dashboard/ps/tambah">Tambah Petugas</a></li>
            </ul>
          </li>
        </ul>
      </div>
    </aside>
    <section class="content">
      <div class="body_scroll">
        <div class="block-header">
          <div class="row">
            <div class="col-lg-7 col-md-6 col-sm-12">
              <h2>Tendaku Dashboard</h2>
              <ul class="breadcrumb">
                <li class="breadcrumb-item">
                  <a href="/petugas/dashboard"><i class="zmdi zmdi-home"></i> Tendaku</a>
                </li>
                <li class="breadcrumb-item">Dashboard</li>
                <li class="breadcrumb-item">User</li>
                <li class="breadcrumb-item active">Tambah</li>
              </ul>
              <button class="btn btn-primary btn-icon mobile_menu" type="button">
                <i class="zmdi zmdi-sort-amount-desc"></i>
              </button>
            </div>
          </div>
        </div>
      </div>

      <div class="container-fluid">
        <div class="row clearfix">
          <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="card">
              <div class="body">
                <form
                  class="form-horizontal"
                  action="/petugas/add/user"
                  method="POST"
                >
                  <div class="row clearfix">
                    <div class="col-lg-2 col-md-2 col-sm-4 form-control-label">
                      <label for="email_address_2">Nama</label>
                    </div>
                    <div class="col-lg-10 col-md-10 col-sm-8">
                      <div class="form-group">
                        <input
                          type="text"
                          id="email_address_2"
                          name="nm_user"
                          class="form-control"
                          placeholder="Nama"
                          required
                        />
                      </div>
                    </div>
                  </div>
                  <div class="row clearfix">
                    <div class="col-lg-2 col-md-2 col-sm-4 form-control-label">
                      <label for="email_address_2">Email</label>
                    </div>
                    <div class="col-lg-10 col-md-10 col-sm-8">
                      <div class="form-group">
                        <input
                          type="email"
                          id="email_address_2"
                          class="form-control"
                          name="email"
                          required
                          placeholder="Email"
                        />
                      </div>
                    </div>
                  </div>
                  <div class="row clearfix">
                    <div class="col-lg-2 col-md-2 col-sm-4 form-control-label">
                      <label for="email_address_2">Alamat</label>
                    </div>
                    <div class="col-lg-10 col-md-10 col-sm-8">
                      <div class="form-group">
                        <textarea
                          rows="5"
                          class="form-control no-resize"
                          name="alamat"
                          placeholder="Alamat...."
                        ></textarea>
                      </div>
                    </div>
                  </div>
                  <div class="row clearfix">
                    <div class="col-lg-2 col-md-2 col-sm-4 form-control-label">
                      <label for="email_address_2">Telepon</label>
                    </div>
                    <div class="col-lg-10 col-md-10 col-sm-8">
                      <div class="form-group">
                        <div class="input-group">
                          <div class="input-group-prepend">
                            <span class="input-group-text" style="font-size: 12px;">+62</span>
                          </div>
                          <input
                            type="number"
                            id="email_address_2"
                            class="form-control"
                            name="tlp"
                            placeholder="Ex: 888888888"
                          />
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="row clearfix">
                    <div class="col-lg-2 col-md-2 col-sm-4 form-control-label">
                      <label for="email_address_2">Kata Sandi</label>
                    </div>
                    <div class="col-lg-10 col-md-10 col-sm-8">
                      <div class="form-group">
                        <input
                          type="password"
                          id="email_address_2"
                          class="form-control"
                          name="password"
                          placeholder="Kata Sandi"
                        />
                      </div>
                    </div>
                  </div>
                  <div class="row clearfix">
                    <div class="col-lg-2 col-md-2 col-sm-4 form-control-label">
                      <label for="email_address_2">Konfirmasi Kata Sandi</label>
                    </div>
                    <div class="col-lg-10 col-md-10 col-sm-8">
                      <div class="form-group">
                        <input
                          type="password"
                          id="email_address_2"
                          class="form-control"
                          name="password2"
                          placeholder="Konfirmasi Kata Sandi"
                        />
                      </div>
                    </div>
                  </div>
                  <div class="row clearfix">
                    <div class="col-sm-8 offset-sm-6">
                      <button
                        type="submit"
                        value="Submit"
                        formmethod="POST"
                        class="btn btn-raised btn-primary btn-lg btn-round waves-effect"
                      >
                        Simpan
                      </button>
                    </div>
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
    <script src="adminbundles/libscripts.bundle.js"></script>
    <!-- Lib Scripts Plugin Js -->
    <script src="adminbundles/vendorscripts.bundle.js"></script>
    <!-- Lib Scripts Plugin Js -->

    <script src="plugins/dropify/js/dropify.min.js"></script>
    <script src="adminbundles/mainscripts.bundle.js"></script>
    <!-- Custom Js -->
    <script src="adminjs/pages/forms/dropify.js"></script>
  </body>
</html>
