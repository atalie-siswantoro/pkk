<!DOCTYPE html>
<html class="no-js" lang="en">
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=Edge" />
    <meta
      content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no"
      name="viewport"
    />

    <title>Dashboard :: Tendaku</title>
    <!-- Favicon-->
    <link rel="icon" href="img/favicon.png" type="image/x-icon" />
    <link rel="stylesheet" href="plugins/bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" href="plugins/jvectormap/jquery-jvectormap-2.0.3.css" />
    <link rel="stylesheet" href="plugins/morrisjs/morris.css" />

    <!-- Custom Css -->
    <link rel="stylesheet" href="admincss/style.min.css" />
  </head>

  <body class="theme-blush">
    <!-- Page Loader -->
    <div class="page-loader-wrapper">
      <div class="loader">
        <div class="m-t-30">
          <img class="zmdi-hc-spin" src="images/loader.svg" width="48" height="48" alt="Aero" />
        </div>
        <p>Please wait...</p>
      </div>
    </div>

    <!-- Overlay For Sidebars -->
    <div class="overlay"></div>

    <div class="navbar-right">
      <ul class="navbar-nav">
        <li>
          <a href="/petugas/logout" class="mega-menu" title="Sign Out"
            ><i class="zmdi zmdi-power"></i
          ></a>
        </li>
      </ul>
    </div>
    <!-- Left Sidebar -->
    <aside id="leftsidebar" class="sidebar">
      <div class="navbar-brand">
        <button class="btn-menu ls-toggle-btn" type="button"><i class="zmdi zmdi-menu"></i></button>
        <a href="/petugas/dashboard"><span class="m-l-10">Tendaku</span></a>
      </div>
      <div class="menu">
        <ul class="list">
          <li>
            <div class="user-info">
              <a class="image" href="profile.html"
                ><img src="images/profile_av.jpg" alt="User"
              /></a>
              <div class="detail">
                <h4><%= userData.nama %></h4>
                <small>Petugas Tendaku</small>
              </div>
            </div>
          </li>
          <li class="active">
            <a href="/petugas/dashboard"><i class="zmdi zmdi-home"></i><span>Dashboard</span></a>
          </li>

          <li>
            <a href="javascript:void(0);" class="menu-toggle"
              ><i class="zmdi zmdi-shopping-cart"></i><span>Tenda</span></a
            >
            <ul class="ml-menu">
              <li><a href="/petugas/dashboard/tenda">Tenda List</a></li>
              <li><a href="/petugas/dashboard/tenda/tambah">Tambah Tenda</a></li>
            </ul>
          </li>
          <li>
            <a href="javascript:void(0);" class="menu-toggle">
              <i class="zmdi zmdi-grid"></i><span>Peminjaman</span></a
            >
            <ul class="ml-menu">
              <li><a href="/petugas/dashboard/peminjaman">Peminjaman List</a></li>
              <li><a href="/petugas/dashboard/peminjaman/tambah">Tambah Peminjaman</a></li>
            </ul>
          </li>
          <li>
            <a href="javascript:void(0);" class="menu-toggle"
              ><i class="zmdi zmdi-accounts-list"></i><span>User</span></a
            >
            <ul class="ml-menu">
              <li><a href="/petugas/dashboard/user">Users List</a></li>
              <li><a href="/petugas/dashboard/user/tambah">Tambah User</a></li>
            </ul>
          </li>
          <li>
            <a href="javascript:void(0);" class="menu-toggle"
              ><i class="zmdi zmdi-accounts-list-alt"></i><span>Petugas</span></a
            >
            <ul class="ml-menu">
              <li><a href="/petugas/dashboard/ps">Petugas List</a></li>
              <li><a href="/petugas/dashboard/ps/tambah">Tambah Petugas</a></li>
            </ul>
          </li>
        </ul>
      </div>
    </aside>

    <!-- Main Content -->
    <section class="content">
      <div class="body_scroll">
        <div class="block-header">
          <div class="row">
            <div class="col-lg-7 col-md-6 col-sm-12">
              <h2>Tendaku Dashboard</h2>
              <ul class="breadcrumb">
                <li class="breadcrumb-item">
                  <a href="/petugas/dashboard"><i class="zmdi zmdi-home"></i> Tendaku</a>
                </li>
                <li class="breadcrumb-item active">Dashboard</li>
              </ul>
              <button class="btn btn-primary btn-icon mobile_menu" type="button">
                <i class="zmdi zmdi-sort-amount-desc"></i>
              </button>
            </div>
          </div>
        </div>
        <div class="container-fluid">
          <div class="row clearfix">
            <div class="col-lg-3 col-md-6 col-sm-6 col-6 text-center">
              <div class="card">
                <div class="body">
                  <input
                    type="text"
                    class="knob"
                    value="<%= dash.user %>"
                    data-linecap="round"
                    data-width="100"
                    data-height="100"
                    data-thickness="0.08"
                    data-fgColor="#00adef"
                    readonly
                  />
                  <p>Users</p>
                </div>
              </div>
            </div>
            <div class="col-lg-3 col-md-6 col-sm-6 col-6 text-center">
              <div class="card">
                <div class="body">
                  <input
                    type="text"
                    class="knob"
                    value="<%= dash.pinjam %>"
                    data-linecap="round"
                    data-width="100"
                    data-height="100"
                    data-thickness="0.08"
                    data-fgColor="#ee2558"
                    readonly
                  />
                  <p>Peminjaman</p>
                </div>
              </div>
            </div>
            <div class="col-lg-3 col-md-6 col-sm-6 col-6 text-center">
              <div class="card">
                <div class="body">
                  <input
                    type="text"
                    class="knob"
                    value="<%= dash.tenda %>"
                    data-linecap="round"
                    data-width="100"
                    data-height="100"
                    data-thickness="0.08"
                    data-fgColor="#8f78db"
                    readonly
                  />
                  <p>Jumlah Tenda</p>
                </div>
              </div>
            </div>
            <div class="col-lg-3 col-md-6 col-sm-6 col-6 text-center">
              <div class="card">
                <div class="body">
                  <input
                    type="text"
                    class="knob"
                    value="<%= dash.tpinjam %>"
                    data-linecap="round"
                    data-width="100"
                    data-height="100"
                    data-thickness="0.08"
                    data-fgColor="#f67a82"
                    readonly
                  />
                  <p>Tenda Dipinjam</p>
                </div>
              </div>
            </div>
          </div>
          <div class="row clearfix">
            <div class="col-lg-8 col-md-12">
              <div class="card product-report">
                <div class="header">
                  <h2><strong>Annual</strong> Report</h2>
                </div>
                <div class="body">
                  <div class="row clearfix">
                    <div class="col-lg-4 col-md-4 col-sm-4">
                      <div class="icon xl-amber m-b-15"><i class="zmdi zmdi-chart-donut"></i></div>
                      <div class="col-in">
                        <small class="text-muted mt-0">Sales Report</small>
                        <h4 class="mt-0">$4,516</h4>
                      </div>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-4">
                      <div class="icon xl-blue m-b-15"><i class="zmdi zmdi-chart"></i></div>
                      <div class="col-in">
                        <small class="text-muted mt-0">Annual Revenue</small>
                        <h4 class="mt-0">$6,481</h4>
                      </div>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-4">
                      <div class="icon xl-purple m-b-15"><i class="zmdi zmdi-card"></i></div>
                      <div class="col-in">
                        <small class="text-muted mt-0">Total Profit</small>
                        <h4 class="mt-0">$3,915</h4>
                      </div>
                    </div>
                  </div>
                  <div id="area_chart"></div>
                </div>
              </div>
            </div>
            <div class="col-lg-4 col-md-12">
              <div class="card">
                <div class="header">
                  <h2><strong>Popular</strong> Products</h2>
                </div>
                <div class="table-responsive">
                  <table class="table table-hover theme-color c_table">
                    <thead>
                      <tr>
                        <th>Tenda</th>
                        <th></th>
                        <th>Dipinjam</th>
                      </tr>
                    </thead>
                    <tbody>
                      <% for(var i = 0; i < populer.length; i++){ %>
                      <tr>
                        <td class="w70">
                          <img class="w50" src="/api/tenda/img/<%= populer[i].kd_tenda %>" alt="" />
                        </td>
                        <td>
                          <a href="javascript:void(0)" class="text-muted"
                            ><%= populer[i].nm_tenda %></a
                          >
                        </td>
                        <td><%= populer[i].dipinjam %>x</td>
                      </tr>
                      <% } %>
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>
          <div class="row clearfix">
            <div class="col-sm-12 col-md-12 col-lg-12">
              <div class="card">
                <div class="header">
                  <h2><strong>Peminjaman</strong> Terbaru</h2>
                </div>
                <div class="table-responsive">
                  <table class="table table-hover c_table">
                    <thead>
                      <tr>
                        <th style="width: 60px;">No</th>
                        <th>Tenda</th>
                        <th>Jumlah</th>
                        <th>Total</th>
                        <th>Kode</th>
                        <th>User</th>
                        <th>Petugas</th>
                        <th>Status</th>
                      </tr>
                    </thead>
                    <tbody>
                      <% peminjaman.map((item, i) => { %>
                      <tr>
                        <td><%= i+1 %></td>
                        <td><%= item.tendaData.nm_tenda %></td>
                        <td><%= item.main.jumlah %></td>
                        <td>Rp. <%= item.main.total %></td>
                        <td><%= item.main.kode %></td>
                        <td><%= item.userData.nm_user %></td>
                        <td>
                          <% if(item.petugasData.nm_petugas){ %> <%= item.petugasData.nm_petugas %>
                          <%}else{ %> Tidak ada <%} %>
                        </td>
                        <td>
                          <% if(item.main.status.code === 0) { %>
                          <span class="badge badge-success"><%= item.main.status.status %></span>
                          <%} else if(item.main.status.code === 1){%>
                          <span class="badge badge-primary"><%= item.main.status.status %></span>
                          <%} else if(item.main.status.code === 2){%>
                          <span class="badge badge-warning"><%= item.main.status.status %></span>
                          <%} else if(item.main.status.code === 3){%>
                          <span class="badge badge-danger"><%= item.main.status.status %></span>
                          <%}else if(item.main.status.code === 4){%>
                          <span class="badge badge-default"><%= item.main.status.status %></span>
                          <%} %>
                        </td>
                      </tr>
                      <% }) %>
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
    <!-- Jquery Core Js -->
    <script src="adminbundles/libscripts.bundle.js"></script>
    <!-- Lib Scripts Plugin Js -->
    <script src="adminbundles/vendorscripts.bundle.js"></script>
    <!-- Lib Scripts Plugin Js -->

    <script src="adminbundles/morrisscripts.bundle.js"></script>
    <!-- Morris Plugin Js -->
    <script src="adminbundles/jvectormap.bundle.js"></script>
    <!-- JVectorMap Plugin Js -->
    <script src="adminbundles/sparkline.bundle.js"></script>
    <!-- Sparkline Plugin Js -->
    <script src="adminbundles/knob.bundle.js"></script>
    <!-- Jquery Knob Plugin Js -->

    <script src="adminbundles/mainscripts.bundle.js"></script>
    <script src="adminjs/pages/ecommerce.js"></script>
    <script src="adminjs/pages/charts/jquery-knob.min.js"></script>
  </body>
</html>
